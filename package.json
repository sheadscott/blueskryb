{
  "name": "bluesky-oauth-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "trigger:dev": "npx trigger.dev@latest dev",
    "trigger:prod": "npx trigger.dev@latest deploy",
    "dev-concurrent": "npx concurrently --kill-others --names \"next,trigger\" --prefix-colors \"yellow,blue\" \"next dev --turbopack\" \"npm run trigger:dev\"",
    "build": "npm run drizzle:dev && next build",
    "start": "next start",
    "lex:generate": "./node_modules/.bin/lex gen-server ./src/lexicon ./lexicons/*",
    "lint": "next lint",
    "format": "prettier --write .",
    "drizzle:dev": "npx drizzle-kit push",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "sb:dev": "open http://127.0.0.1:54323",
    "sb:staging": "open https://supabase.com/dashboard/project/nnmhzennsdxwcqghureb",
    "vercel:open": "open https://vercel.com/shea-scotts-projects/blueskryb/deployments",
    "vercel:view:dev": "open https://blueskryb-git-dev-shea-scotts-projects.vercel.app"
  },
